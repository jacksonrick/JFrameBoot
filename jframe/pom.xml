    <modelVersion>4.0.0</modelVersion>
    <artifactId>jframe</artifactId>
    <groupId>com.jf</groupId>
    <version>4.0</version>
    <packaging>pom</packaging>

    <modules>
        <module>../jframe-utils</module>
        <module>../jframe-model</module>
        <module>../jframe-core</module>
        <module>../jframe-web-manage</module>
        <module>../jframe-web-front</module>
        <module>../jframe-web-app</module>
        <module>../jframe-web-shop</module>
        <module>../cloud-server</module>
    </modules>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>1.5.9.RELEASE</version>
        <relativePath/>
